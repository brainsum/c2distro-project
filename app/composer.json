{
    "name": "brainsum/c2-app",
    "description": "App for c2.",
    "license": "GPL-2.0-or-later",
    "type": "project",
    "require": {
        "php": "^8.1",
        "brainsum/c2distro": "2.0.x-dev",
        "composer/installers": "^2.1",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0.1",
        "drupal/classy": "^1.0",
        "drupal/core-composer-scaffold": "^10.0.0",
        "drupal/core-recommended": "^10.0.0",
        "drupal/exception_mailer": "^4.0",
        "drupal/maillog": "^1.0.0",
        "drupal/openid_connect": "^3.0@alpha",
        "drush/drush": "^11.4.0",
        "mglaman/composer-drupal-lenient": "^1.0",
        "oomphinc/composer-installers-extender": "^2.0.0",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "brainsum/drupal-dev-tools": "^4.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "drupal/core-dev": "^10.0.0",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/devel_php": "^1.3.0",
        "weitzman/drupal-test-traits": "^2.0",
        "weitzman/logintrait": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c2d\\Tests\\": "tests/src/"
        },
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "phpro/grumphp": true,
            "oomphinc/composer-installers-extender": true,
            "phpstan/extension-installer": true,
            "mglaman/composer-drupal-lenient": true,
            "ergebnis/composer-normalize": true,
            "brainsum/drupal-dev-tools": true
        },
        "apcu-autoloader": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ]
    }
}
